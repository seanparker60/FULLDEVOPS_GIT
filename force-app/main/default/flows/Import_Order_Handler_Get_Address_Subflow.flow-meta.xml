    <description>This flow queries the custom address, verifies if it can find a match. If not it creates &amp; validates a new address if no match is found</description>
    <environments>Default</environments>
    <formulas>
        <description>Street + HouseNumber + HouseNUmberAddition + PostalCode + Country</description>
        <name>form_AddressLabel</name>
        <dataType>String</dataType>
        <expression>{!var_Street}&amp;&apos; &apos;&amp;{!var_HouseNumber}&amp;{!var_HouseNumberAddition}&amp; &quot;, &quot;&amp;{!var_PostalCode}&amp;&quot;, &quot;&amp;{!var_Country}</expression>
    </formulas>
    <formulas>
        <description>Set City value to upper case</description>
        <name>formattedCity</name>
        <dataType>String</dataType>
        <expression>IF({!var_Country} = &quot;Netherlands&quot;, UPPER({!var_City}), {!var_City})</expression>
    </formulas>
    <formulas>
        <name>formattedHouseNumberAddition</name>
        <dataType>String</dataType>
        <expression>TRIM(UPPER({!var_HouseNumberAddition}))</expression>
    </formulas>
    <formulas>
        <description>Removed whitespaces</description>
        <name>formattedPostalCode</name>
        <dataType>String</dataType>
        <expression>UPPER(TRIM(SUBSTITUTE({!var_PostalCode}, &quot; &quot;, &quot;&quot;)))</expression>
    </formulas>
